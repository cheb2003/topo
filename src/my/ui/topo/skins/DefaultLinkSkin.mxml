<?xml version="1.0"?>
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark" width="153" height="133">
    <fx:Metadata>
        <![CDATA[
        [HostComponent("my.ui.topo.Link")]
        ]]>
    </fx:Metadata>
    <s:states>
        <s:State name="normal"/>
        <s:State name="selected"/>
        <s:State name="mouseOver"/>
    </s:states>
	<s:Group id="contentGroup">
		
    <s:Line xFrom="{hostComponent.startNode.x}" xTo="{hostComponent.endNode.x}" yFrom="{hostComponent.startNode.y}" yTo="{hostComponent.endNode.y}">
		<s:stroke>
			<s:SolidColorStroke color="0x00ff33" weight="1" caps="square"/>
		</s:stroke>
	</s:Line>
	</s:Group>
</s:SparkSkin>
